{
  "name": "react-server-render-starter-kit",
  "version": "0.1.1",
  "description": "a react server render app starter kit with redux/react-router/async/await/webpack support",
  "main": "index.js",
  "scripts": {
    "test": "npm run e2e",
    "e2e": "node test/e2e/runner.js",
    "start": "run-p dev:server dev:client build:client build:server",
    "build:server": "NODE_ENV=production ./node_modules/.bin/webpack --harmony --config scripts/webpack.config.server -p --progress",
    "build:client": "rm -rf public/ && NODE_ENV=production ./node_modules/.bin/webpack -p --progress",
    "build": "run-p build:client build:server",
    "dev:server": "./node_modules/.bin/nodemon server/index.js --config scripts/nodemon.json",
    "dev:server-debug": "./node_modules/.bin/nodemon --debug-brk server/index.js --config scripts/nodemon.json",
    "dev:client": "./node_modules/.bin/webpack-dev-server --progress --colors --inline --content-base public/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zxdong262/react-server-render-starter-kit.git"
  },
  "keywords": [
    "react",
    "server render",
    "starter"
  ],
  "author": {
    "name": "ZHAO Xudong",
    "email": "zxdong@gmail.com",
    "url": "http://html5beta.com"
  },
  "dependencies": {
    "dolor": "^0.2.1",
    "isomorphic-fetch": "^2.2.1",
    "koa": "^1.2.4",
    "koa-bodyparser": "^2.2.0",
    "koa-conditional-get": "^1.0.3",
    "koa-etag": "^2.1.1",
    "koa-mount": "^1.3.0",
    "koa-pug": "^3.0.0-0",
    "koa-router": "^5.4.0",
    "koa-static": "^2.0.0",
    "lodash": "^4.16.2",
    "react": "15.3.1",
    "react-dom": "15.3.1"
  },
  "devDependencies": {
    "babel-core": "6.14.0",
    "babel-loader": "6.2.5",
    "babel-plugin-transform-es2015-modules-factory": "6.0.0",
    "babel-plugin-transform-es2015-modules-umd": "6.12.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-0": "6.5.0",
    "babel-preset-stage-1": "6.13.0",
    "babel-preset-stage-2": "6.13.0",
    "chai": "3.5.0",
    "chai-as-promised": "5.3.0",
    "chromedriver": "2.21.2",
    "co": "4.6.0",
    "cross-spawn": "4.0.0",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "json-loader": "^0.5.4",
    "mocha": "3.0.2",
    "nightwatch": "0.9.8",
    "nodemon": "^1.10.2",
    "npm-run-all": "^3.1.0",
    "open-browser-webpack-plugin": "0.0.2",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-pagenav": "0.4.1",
    "react-redux": "4.4.5",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.6",
    "react-server": "0.4.7",
    "redux": "3.6.0",
    "redux-async-connect": "^1.0.0-rc4",
    "redux-factories": "0.2.1",
    "redux-thunk": "^2.1.0",
    "request": "2.74.0",
    "selenium-server": "2.53.1",
    "style-loader": "^0.13.1",
    "stylus-loader": "^2.3.1",
    "superagent": "^2.2.0",
    "unminified-webpack-plugin": "^1.1.0",
